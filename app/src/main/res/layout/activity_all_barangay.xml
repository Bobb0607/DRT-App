<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    tools:context=".Categories">


    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints">


        <RelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView1"
                android:layout_width="350dp"
                android:layout_height="180dp"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="23dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="20dp"
                android:foregroundGravity="center"
                android:onClick="Mountains"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints,DuplicateClickableBoundsCheck,DuplicateClickableBoundsCheck">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/mtmavio2">

                </ImageView>

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="5dp"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Mountains"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView2"
                android:layout_width="170dp"
                android:layout_height="200dp"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/materialCardView1"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="false"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_toEndOf="@+id/materialCardView5"
                android:foregroundGravity="center"
                android:onClick="Caves"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints,DuplicateClickableBoundsCheck">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/puningcave3">

                </ImageView>

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Caves"
                    android:paddingLeft="5dp"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView3"
                android:layout_width="170dp"
                android:layout_height="200dp"
                android:layout_below="@+id/materialCardView5"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:foregroundGravity="center"
                android:onClick="Hills"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/tilapilonhills">

                </ImageView>

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Hills"
                    android:paddingLeft="5dp"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView4"
                android:layout_width="170dp"
                android:layout_height="300dp"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/materialCardView2"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_toEndOf="@+id/materialCardView3"
                android:foregroundGravity="center"
                android:onClick="Infrastructures"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/simbahangbato3">

                </ImageView>

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Infrastructures"
                    android:paddingLeft="5dp"
                    android:textColor="#FFFFFF"
                    android:textSize="22sp" />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView5"
                android:layout_width="170dp"
                android:layout_height="300dp"
                android:layout_below="@+id/materialCardView1"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:foregroundGravity="center"
                android:onClick="Falls"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints,DuplicateClickableBoundsCheck">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/talonnieba">

                </ImageView>

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Falls"
                    android:paddingLeft="5dp"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView6"
                android:layout_width="170dp"
                android:layout_height="200dp"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/materialCardView9"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="false"
                android:layout_marginStart="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_toEndOf="@+id/materialCardView7"
                android:foregroundGravity="center"
                android:onClick="Lake"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/palanguyan">

                </ImageView>

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:paddingLeft="5dp"
                    android:text="Lakes"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView7"
                android:layout_width="170dp"
                android:layout_height="200dp"
                android:layout_below="@+id/materialCardView9"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:foregroundGravity="center"
                android:onClick="Campsites"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/cafe2">

                </ImageView>

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Campsites"
                    android:paddingLeft="5dp"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView8"
                android:layout_width="350dp"
                android:layout_height="180dp"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/materialCardView7"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="false"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="21dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="80dp"
                android:foregroundGravity="center"
                android:onClick="Springs"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                app:cardElevation="10dp"
                tools:ignore="MissingConstraints">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/malangaanspring">

                </ImageView>

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Springs"
                    android:paddingLeft="5dp"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/materialCardView9"
                android:layout_width="350dp"
                android:layout_height="180dp"
                android:layout_alignWithParentIfMissing="true"
                android:layout_below="@+id/materialCardView4"
                android:layout_alignParentStart="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="0dp"
                android:foregroundGravity="center"
                android:onClick="Mountains"
                app:cardBackgroundColor="#FFFFFF"
                app:cardCornerRadius="10dp"
                tools:ignore="MissingConstraints,DuplicateClickableBoundsCheck,DuplicateClickableBoundsCheck">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:onClick="Rivers"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/tangke">

                </ImageView>

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left|end"
                    android:fontFamily="sans-serif-condensed-medium"
                    android:text="Rivers"
                    android:paddingLeft="5dp"
                    android:textAlignment="viewEnd"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />
            </com.google.android.material.card.MaterialCardView>

        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="90dp"
        android:src="@drawable/ic_baseline_question_mark_24"
        app:backgroundTint="#79FFFFFF"
        app:tint="@color/white" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignBottom="@id/scrollView"
        android:background="#0099d1"
        app:itemIconTint="@drawable/selector"
        app:itemTextColor="@drawable/selector"
        app:layout_constraintVertical_bias="1.0"
        app:menu="@menu/manu_navigation"
        tools:ignore="NotSibling" />
</RelativeLayout>